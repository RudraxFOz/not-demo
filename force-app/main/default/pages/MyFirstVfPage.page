<apex:page sidebar="true" showHeader="true" standardController="Bank_Account__c">
 <apex:form >
     <apex:pageBlock title="Edit Contact">
         <apex:pageBlockSection columns="1">
            <apex:inputField value="{!Bank_Account__c.FistName__c}"/> 
            <apex:inputField value="{!Bank_Account__c.Satus__c}"/>
            <apex:inputField value="{!Bank_Account__c.Type_Of_Account__c}"/>
            <apex:inputField value="{!Bank_Account__c.Name}"/>
         </apex:pageBlockSection>
         <apex:pageBlockSection columns="1">
            <apex:inputField value="{!Bank_Account__c.Amount__c}"/> 
            <apex:inputField value="{!Bank_Account__c.Atyo__c}"/>
         </apex:pageBlockSection>
         <apex:pageBlockButtons >
            <apex:commandButton action="{!save}" value="Save"/>
         </apex:pageBlockButtons>
     </apex:pageBlock>
 </apex:form>
</apex:page>