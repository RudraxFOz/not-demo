<apex:page extensions="AllInOneControl" standardController="AllInOne__c">
    <apex:form >
        
        <apex:pageBlock title="Edit Contact">

            <apex:pageBlockSection columns="1">
                <apex:inputField value="{!c.First_Name__c}"/>
                <apex:inputField value="{!c.Last_Name__c}"/>
                <apex:inputField value="{!c.EmailId__c}"/>
                <apex:inputField value="{!c.Phone_No__c}"/>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
                <apex:commandButton action="{!save}" value="Save"/>
               
            </apex:pageBlockButtons>
        </apex:pageBlock>
        
    </apex:form>
</apex:page>